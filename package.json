{
  "name": "kilo-emitter",
  "version": "0.1.0",
  "description": "A sub 1Kb event emitter class with broader functionality.",
  "homepage": "https://github.com/n1kk/kilo-emitter",
  "repository": {
    "type": "git",
    "url": "https://github.com/n1kk/kilo-emitter.git"
  },
  "bugs": {
    "url": "https://github.com/n1kk/kilo-emitter/issues"
  },
  "main": "dist/Emitter.js",
  "module": "dist/Emitter.es6.js",
  "browser": "dist/Emitter.es3.browser.js",
  "types": "dist/emitter.es3.js",
  "scripts": {
    "build": "gulp dist",
    "readme": "gulp readme",
    "test": "mocha -r ts-node/register -r jsdom-global/register test/**/*.spec.ts",
    "test:wdio": "wdio wdio.conf.js",
    "test:cov": "nyc npm test",
    "all": "npm run build && npm run test:cov && npm run test:wdio && npm run readme"
  },
  "nyc": {
    "include": [
      "src/Emitter.ts",
      "dist/emitter.es3.browser.js"
    ],
    "exclude": [
      "typings",
      "**/*.spec.ts",
      "test/**/*"
    ],
    "extension": [
      ".ts",
      ".js"
    ],
    "reporter": [
      "lcov",
      "json-summary",
      "text-summary"
    ],
    "all": true,
    "sourceMap": true,
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "author": "n1kk",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.30",
    "@types/sinon": "^4.3.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "del": "^3.0.0",
    "fs-extra": "^5.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-filter": "^5.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-save": "^1.3.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typedoc": "^2.2.0",
    "gulp-typescript": "^4.0.1",
    "gulp-uglify": "^3.0.0",
    "gulp-uglify-es": "^1.0.1",
    "gzip-size": "^4.1.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.0",
    "nyc": "^11.6.0",
    "phantomjs": "^2.1.7",
    "prettysize": "^1.1.0",
    "rollup": "^0.56.5",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.2.0",
    "sinon": "^4.4.6",
    "slimerjs": "^0.10.3",
    "strip-indent": "^2.0.0",
    "table": "^4.0.3",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "typescript": "^2.7.2",
    "wdio-concise-reporter": "^0.1.2",
    "wdio-dot-reporter": "0.0.9",
    "wdio-json-reporter": "^0.3.1",
    "wdio-mocha-framework": "^0.5.11",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.2",
    "webdriverio": "^4.12.0"
  }
}
